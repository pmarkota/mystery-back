{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "https://raw.githubusercontent.com/vercel/vercel/main/packages/frameworks/logo.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
